{
  "name": "dredd",
  "version": "13.1.0",
  "description": "HTTP API Testing Framework",
  "main": "build/index.js",
  "bin": {
    "dredd": "bin/dredd"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "npm run build -- --watch",
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . bin/dredd",
    "prettify": "prettier --write lib/**/*.js test/**/*.js",
    "prettify:check": "prettier --check lib/**/*.js test/**/*.js",
    "test": "mocha \"./test/**/*-test.js\"",
    "test:smoke": "bash ./scripts/smoke.sh",
    "e2e": "yarn e2e:apib && yarn e2e:openapi2",
    "e2e:apib": "cucumber-js",
    "e2e:openapi2": "cucumber-js --world-parameters=\"{\\\"apiDescriptionFormat\\\": \\\"application/swagger+yaml\\\"}\"",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apiaryio/dredd"
  },
  "files": [
    "bin",
    "build",
    "options.json",
    "README.md"
  ],
  "dependencies": {
    "async": "3.2.0",
    "caseless": "0.12.0",
    "chai": "4.2.0",
    "clone": "2.1.2",
    "cross-spawn": "7.0.0",
    "dredd-transactions": "^9.2.0",
    "gavel": "^9.1.1",
    "glob": "7.1.6",
    "html": "1.0.0",
    "htmlencode": "0.0.4",
    "inquirer": "7.1.0",
    "js-yaml": "3.13.1",
    "make-dir": "3.0.0",
    "markdown-it": "10.0.0",
    "optimist": "0.6.1",
    "proxyquire": "2.1.3",
    "ramda": "0.27.0",
    "request": "2.88.0",
    "spawn-args": "0.2.0",
    "untildify": "4.0.0",
    "uuid": "3.3.3",
    "which": "1.3.1",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/clone": "0.1.30",
    "@types/mocha": "5.2.7",
    "@types/proxyquire": "1.3.28",
    "@types/ramda": "0.27.3",
    "@types/which": "1.3.2",
    "body-parser": "1.19.0",
    "coffeescript": "2.4.1",
    "cucumber": "6.0.5",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.18.2",
    "express": "4.17.1",
    "mocha": "7.1.1",
    "nock": "10.0.6",
    "prettier": "1.18.2",
    "ps-node": "0.1.6",
    "sinon": "9.0.1",
    "ts-node": "8.7.0",
    "typescript": "3.8.3"
  },
  "keywords": [
    "api",
    "test",
    "testing",
    "documenation",
    "integration",
    "acceptance"
  ],
  "author": "Apiary Czech Republic, s.r.o. <support@apiary.io>",
  "license": "MIT",
  "homepage": "https://github.com/apiaryio/dredd/tree/master/packages/dredd"
}
